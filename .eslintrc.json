{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": ["standard", "plugin:cypress/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "parser": "@babel/eslint-parser",
    "rules": {
    "semi": "off",
    "space-before-function-paren": "off",
    "no-return-assign": "off"
  }
}
